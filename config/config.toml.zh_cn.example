# https://toml.io/en/v1.0.0
# 注意：TOML 不是 Python。请不要在此处使用 Python 语法。
# 注意：TOML 中的布尔值必须是小写。
# 如果您不需要某些功能，可注释或删除相应的键。

# 密钥配置部分，此处的值若意外出现在发送的消息中，机器人会尝试拦截。但请务必提防泄露。
[secret]

# 数据库连接字符串，请使用 SQLAlchemy 支持的数据库连接字符串格式。
db_path = "sqlite:///database/save.db"

# 阿里云内容安全服务的 Access Key ID 与 Access Key Secret。
check_access_key_id =
check_access_key_secret =

# WebRender 服务的 URL 地址。
web_render =

# 代理地址
proxy =

# QQ 机器人（官方）的 AppID 与 Secret。
qq_bot_appid =
qq_bot_secret =

# Discord 机器人的 Token。
discord_token =

# Telegram 机器人的 Token。
telegram_token =

# Kook 机器人的 Token。
kook_token =

# Matrix 机器人的 Device ID、Token 与 Megolm Backup Passphrase。
matrix_device_id =
matrix_token =
matrix_megolm_backup_passphrase =

# curseforge 的 API Key。（用于mod_dl模块）
curseforge_api_key =

# diving fish 查分器的开发者 Token。（用于maimai模块）
diving_fish_developer_token =

# exchange_rate 的 API Key。（用于exchange_rate模块，计算汇率用）
exchange_rate_api_key =

# ncmusic 的 API地址。（用于ncmusic模块，网易云音乐查询）
ncmusic_api =

# openai 的 API Key。（用于ask模块，人工智能对话）
openai_api_key =

# osu! 的 API Key。（用于osu模块，查询 osu! 数据）
osu_api_key =

# wolframalpha 的 AppID。（用于wolframalpha模块，查询 WolframAlpha 数据）
wolfram_alpha_appid =

# 内置API的认证密钥，随意填写即可。
jwt_secret =

# 用于加密敏感信息的密钥（使用 ff3 算法进行加密，部分场景可能会使用到）。
ff3_key = 0AF6C4788B86F4E2FB97215E6E4A5EB0
ff3_tweak = 6520BD033E2928

# 一些基础的配置部分，此处填写的值可在消息中以明文形式展示。请不要在此部分填写敏感信息。
[cfg]

# API 服务的端口号。
api_port = 5000

# 机器人的基础超级用户列表，此处填写的值将会在机器人启动时被加载进入数据库。
base_superuser = ["QQ|2596322644",]

# 是否启用调试模式，启用后会输出更多的日志信息。
debug = false

# 命令前缀，此处填写的值将会被识别为命令前缀。
command_prefix = ["~", "～",]

# 确认命令的关键词，此处填写的值将会被识别为确认命令。
confirm_command = ["是", "对", "對", "yes", "Yes", "YES", "y", "Y",]

# 无视的发送者列表，匹配到此列表中的发送者将会被忽略。
ignored_sender = ["QQ|2854196310",]

# 禁用的机器人列表，匹配到此列表中的机器人将会被禁用。
disabled_bots = ["",]

# 反馈目标列表，此处填写的值将会被识别为反馈目标。（在机器人执行命令出错时，机器人将会把报错信息发送至对象）
report_targets = ["",]

# 机器人的默认语言环境。
locale = "zh_cn"

# 机器人的默认时区偏移量。
timezone_offset = "+8"

# bug 反馈地址。
bug_report_url = "https://s.wd-ljt.com/botreportbug"

# 捐赠地址。
donate_url = "https://afdian.com/a/teahouse"

# 帮助地址。
help_url = "https://bot.teahouse.team"

# 模块帮助地址。
help_page_url = "https://bot.teahouse.team/wiki/${module}"

# 问题反馈地址。
issue_url = "https://github.com/Teahouse-Studios/akari-bot/issues/new/choose"

# 本地化地址。
locale_url = "https://www.crowdin.com/project/akari-bot"

# wiki 模块白名单申请地址。
wiki_whitelist_url = "https://s.wd-ljt.com/botwhitelist"

# 机器人 QQ 号（部署 QQ 机器人（使用 onebot11（aiocqhttp） 部署时）使用）
qq_account = 2314163511

# Aiocqhttp WebSocket 主服务器的地址。（onebot 协议端请使用反向 Websocket 方式进行连接）
qq_host = "127.0.0.1:11451"

# Aiocqhttp 对接的 onebot 协议的框架类型。
qq_frame_type = "go-cqhttp"

# 使用 onebot 协议时，是否启用临时会话。
qq_disable_temp_session = false

# 使用 onebot 协议时，是否自动同意好友请求。
qq_allow_approve_friend = true

# 使用 onebot 协议时，是否自动同意群邀请。
qq_allow_approve_group_invite = true

# 使用 onebot 协议时，是否启用自身消息监听。
qq_enable_listening_self_message = false

# 使用 onebot 协议时，消息处理失败时向消息挂上的回应 emoji，需协议端支持。ID 详见：https://bot.q.qq.com/wiki/develop/api/openapi/emoji/model.html#Emoji
qq_limited_emoji = 10060

# 使用 onebot 协议时，正在处理消息时向消息挂上的回应 emoji id，需协议端支持。ID 详见：https://bot.q.qq.com/wiki/develop/api/openapi/emoji/model.html#Emoji
qq_typing_emoji = 181

# 使用 QQ 机器人（官方）时，是否启用发送 URL。
qq_bot_enable_send_url = false

# 使用 QQ 机器人（官方）时，是否为私域机器人。
qq_private_bot = false

# 是否允许机器人请求本地私有 IP 地址。（防止可能的信息泄露）
allow_request_private_ip = false

# 是否允许重载基础模块。
allow_reload_base = false

# 是否无需发送者确认消息再执行命令。
no_confirm = false

# 是否启用更慢的计划任务调度器。（减少请求压力用）
slower_schedule = false

# 是否使用 secrets 模块的随机数生成器。
use_secrets_random = true

# 是否在展示版本信息时显示 commit URL。
enable_commit_url = true

# 是否启用文字过滤检测，需要配置阿里云内容安全服务的 Access Key ID 与 Access Key Secret。
enable_dirty_check = true

# 当文字过滤检测失败时，是否 rickroll 对象。
enable_rickroll = true
rickroll_msg = "https://wdf.ink/6OUp"

# 是否启用玩笑。
enable_joke = false
shuffle_rate = 0.1

# 是否启用 URLManager，用于 URL 跳板用。（声明此 URL 非官方链接等）
enable_urlmanager = true

# 是否启用内置的 Tos 检查规则。
enable_tos = false

# 违反 Tos 时的警告次数。
tos_warning_counts = 5

# 违反 Tos 时的临时封禁时间。
tos_temp_ban_time = 300

# 是否启用内置的统计功能。
enable_analytics = true

# 是否启用 eval 模块。
enable_eval = true

# 是否启用花瓣功能。
enable_petal = true

# 是否允许获取花瓣。
enable_get_petal = true

# 单日获取花瓣上限。
gained_petal_limit = 10

# 单日丢失花瓣上限。
lost_petal_limit = 5

# coin 模块可抛硬币的上限。
coin_limit = 10000

# coin 模块抛硬币时正面的概率。
coin_faceup_rate = 4997

# coin 模块抛硬币时反面的概率。
coin_facedown_rate = 4997

# dice 模块的一次摇动最多的骰子数量。
dice_limit = 100

# dice 模块一次命令最多的摇动次数。
dice_roll_limit = 10

# dice 模块的输出的最多数据量。
dice_output_count = 50

# dice 模块的输出的最大长度。
dice_output_len = 200

# dice 模块的 n 次投掷的骰子的总量超过该值时将不再显示详细信息。
dice_detail_count = 5

# dice 模块的骰子表达式最多的项数。
dice_count_limit = 10

# ncmusic 模块是否启用卡片消息。
ncmusic_enable_card = false

# wordle 模块是否禁用图片。
wordle_disable_image = false

# matrix 机器人的设备名称，homeserver 与用户 ID。
matrix_device_name =
matrix_homeserver =
matrix_user = "@akaribot:matrix.org"

# 本地 WebRender 服务的地址。
web_render_local =
